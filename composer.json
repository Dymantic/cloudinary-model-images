{
    "name": "dymantic/cloudinary-model-images",
    "description": "Assosiate cloudinary uploads with laravel models",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Michael Joyner",
            "email": "joyner.michael@gmail.com"
        }
    ],
    "require": {
        "cloudinary/cloudinary_php": "^1.16",
        "spatie/image": "^1.7"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5",
        "orchestra/testbench": "^4.6"
    },
    "autoload": {
        "psr-4": {
            "Dymantic\\CloudinaryModelImages\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dymantic\\CloudinaryModelImages\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Dymantic\\CloudinaryModelImages\\CloudinaryModelImagesServiceProvider"
            ]
        }
    }
}
